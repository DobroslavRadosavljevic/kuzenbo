{
  "name": "kuzenbo",
  "version": "0.0.3",
  "description": "React UI components, hooks, and utilities library.",
  "keywords": [
    "react",
    "react-components",
    "react-hooks",
    "react-library",
    "typescript",
    "ui-components",
    "utilities"
  ],
  "homepage": "https://github.com/DobroslavRadosavljevic/kuzenbo#readme",
  "bugs": {
    "url": "https://github.com/DobroslavRadosavljevic/kuzenbo/issues"
  },
  "license": "MIT",
  "author": "Dobroslav Radosavljevic <dobroslav.radosavljevic@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/DobroslavRadosavljevic/kuzenbo"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./styles.css": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown && cp src/styles.css dist/styles.css",
    "dev": "tsdown --watch",
    "test": "bun test",
    "lint": "bun ultracite check",
    "format": "bun ultracite fix",
    "typecheck": "tsc --noEmit",
    "quality": "bun run lint && bun run typecheck && bun run build && bun test",
    "prepare": "husky",
    "prepublishOnly": "bun run build",
    "bump": "bumpp --changelog",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.5",
    "tailwind-variants": "^3.2.2",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@happy-dom/global-registrator": "^20.5.0",
    "@storybook/addon-a11y": "^10.2.7",
    "@storybook/addon-themes": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.8",
    "@types/culori": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "bumpp": "^10.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "motion": "^12.33.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "ultracite": "7.1.5"
  },
  "peerDependencies": {
    "cmdk": "^1.1.1",
    "country-flag-icons": "^1.6.12",
    "culori": "^4.0.2",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "frimousse": "^0.3.0",
    "input-otp": "^1.4.2",
    "media-chrome": "^4.17.2",
    "qrcode": "^1.5.4",
    "react": "^18.0.0 || ^19.0.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-dropzone": "^14.4.0",
    "react-fast-marquee": "^1.6.5",
    "react-resizable-panels": "^4.6.1",
    "react-textarea-autosize": "^8.5.9",
    "recharts": "^2.11.0",
    "vaul": "^1.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "bun run format"
    ],
    "*.{json,md,yml,yaml}": [
      "bun run format"
    ]
  },
  "engines": {
    "bun": ">=1.3.6",
    "node": ">=22"
  }
}
