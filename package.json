{
  "name": "kuzenbo",
  "version": "0.0.1",
  "description": "React UI components, hooks, and utilities library.",
  "keywords": [
    "react",
    "react-components",
    "react-hooks",
    "react-library",
    "typescript",
    "ui-components",
    "utilities"
  ],
  "homepage": "https://github.com/DobroslavRadosavljevic/kuzenbo#readme",
  "bugs": {
    "url": "https://github.com/DobroslavRadosavljevic/kuzenbo/issues"
  },
  "license": "MIT",
  "author": "Dobroslav Radosavljevic <dobroslav.radosavljevic@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/DobroslavRadosavljevic/kuzenbo"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./styles.css": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown && cp src/styles.css dist/styles.css",
    "dev": "tsdown --watch",
    "test": "bun test",
    "lint": "bun ultracite check",
    "format": "bun ultracite fix",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "bun run build",
    "bump": "bumpp --changelog",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@hugeicons-pro/core-stroke-rounded": "^3.1.0",
    "@hugeicons/react": "^1.1.5",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@happy-dom/global-registrator": "^20.5.0",
    "@storybook/addon-a11y": "^10.2.7",
    "@storybook/addon-themes": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.8",
    "@types/culori": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "bumpp": "^10.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "motion": "^12.33.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "ultracite": "7.1.5"
  },
  "peerDependencies": {
    "cmdk": "",
    "country-flag-icons": "",
    "culori": "",
    "embla-carousel": "",
    "embla-carousel-react": "",
    "frimousse": "",
    "input-otp": "",
    "media-chrome": "",
    "qrcode": "",
    "react": "^18.0.0 || ^19.0.0",
    "react-day-picker": "",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-dropzone": "",
    "react-fast-marquee": "",
    "react-resizable-panels": "",
    "react-textarea-autosize": "",
    "recharts": "",
    "vaul": ""
  },
  "peerDependenciesMeta": {
    "cmdk": {
      "optional": true
    },
    "country-flag-icons": {
      "optional": true
    },
    "culori": {
      "optional": true
    },
    "embla-carousel": {
      "optional": true
    },
    "embla-carousel-react": {
      "optional": true
    },
    "frimousse": {
      "optional": true
    },
    "input-otp": {
      "optional": true
    },
    "media-chrome": {
      "optional": true
    },
    "qrcode": {
      "optional": true
    },
    "react-day-picker": {
      "optional": true
    },
    "react-dropzone": {
      "optional": true
    },
    "react-fast-marquee": {
      "optional": true
    },
    "react-resizable-panels": {
      "optional": true
    },
    "react-textarea-autosize": {
      "optional": true
    },
    "recharts": {
      "optional": true
    },
    "vaul": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "bun run format"
    ],
    "*.{json,md,yml,yaml}": [
      "bun run format"
    ]
  },
  "engines": {
    "bun": ">=1.3.6",
    "node": ">=22"
  }
}
